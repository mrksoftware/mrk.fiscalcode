<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>mrksoftware</Authors>
    <PackageReleaseNotes></PackageReleaseNotes>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageTags>codice,fiscale,codicefiscale,codice-fiscale,fiscal,code</PackageTags>
    <Description>Copied from https://github.com/mtpi/CodiceFiscale and migrated to .NET Core</Description>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\data.xml" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Assets\data.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
  </ItemGroup>

</Project>
